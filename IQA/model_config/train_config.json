{
  "data_directory": "data/LIVE2_LIVEitW/",
  "train_directory": "train",
  "val_directory": "val",
  "train_lb": "train_labels.csv",
  "val_lb": "val_labels.csv",
  "image_size": [
    384,
    512
  ],
  "crop_image": false,
  "augment": true,
  "batch_size": 16,
  "epoch_size": 10,
  "continue_train": {
    "from_epoch": 60,
    "from_weights": "trained_models/resnet50_LIVE2_LIVEitW_bsz16_MSE/best_model2.h5"
  },
  "lr": {
    "name": "constant",
    "value": 1e-5
  },
  "loss": {
    "name": "mse"
  },
  "callbacks": {
    "tensorboard_checkpoint": {
      "log_dir": "logs",
      "histogram_freq": 1
    },
    "best_checkpoint": {
      "ckpt_dir": "trained_models/resnet50_LIVE2_LIVEitW_bsz16_MSE",
      "monitor": "val_loss",
      "mode": "min",
      "save_best_only": true,
      "save_weights_only": true
    }
  }
}
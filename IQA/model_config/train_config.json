{
  "data_directory": "data/KonIQ-10K/",
  "train_directory": "train",
  "val_directory": "val",
  "train_lb": "train_labels.csv",
  "val_lb": "val_labels.csv",
  "augment": true,
  "batch_size": 16,
  "epoch_size": 40,
  "continue_train": {
    "from_epoch": 0,
    "from_weights": ""
  },
  "lr": {
    "name": "constant",
    "value": 1e-4
  },
  "loss": {
    "name": "mse"
  },
  "callbacks": {
    "tensorboard_checkpoint": {
      "log_dir": "logs",
      "histogram_freq": 1
    },
    "best_checkpoint": {
      "ckpt_dir": "trained_models/resnet50_KonIQ-10K_256x256_bsz16_MSE/02",
      "monitor": "val_loss",
      "mode": "min",
      "save_best_only": true,
      "save_weights_only": true
    }
  }
}